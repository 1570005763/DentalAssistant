<!--pages/success/success.wxml-->
<view class="page">
  <view class="weui-msg">
    <view class="weui-msg__icon-area">
      <icon type="success" size="93"></icon>
    </view>
    <view class="weui-msg__text-area">
      <view class="weui-msg__title">提交成功</view>
      <view class="weui-msg__desc">已成功提交{{title}}</view>
    </view>
    <view class="weui-msg__opr-area">
      <view class="weui-btn-area">
        <button class="weui-btn" type="primary" bindtap="bindComfirm">确认</button>
        <button class="weui-btn" type="default" bindtap="bindBack">退回登录页</button>
      </view>
    </view>
  </view>
</view>
